<script>

  // Get the modal, button that opens the modal, and the <span> element that closes the modal
  var modal = document.getElementById("myModal");
  var btn = document.querySelector(".image-button");
  var span = document.getElementsByClassName("close")[0];

  // When the button is clicked, open the modal
  btn.onclick = function () {
    modal.style.display = "block";
  }

  // When <span> is clicked, close the modal
  span.onclick = function () {
    modal.style.display = "none";
  }

  // When anywhere outside of the modal is clicked, close it
  window.onclick = function (event) {
    if (event.target == modal) {
      modal.style.display = "none";
    }
  }

  /* Magnifying */
  document.querySelector(".btn-search").addEventListener("click", function () {
    document.querySelector(".input-search").focus();
  });

  // Update the current slider value (each time you drag the slider handle)
  document.getElementById("priceSlider").oninput = function () {
    document.getElementById("priceLabel").innerHTML = this.value;
    // Here you can also call a function to sort your products based on the selected price
  }

  function showProductDetails(event, productId) {
    event.preventDefault();  // Get the product card using the data attribute
    let productCard = document.querySelector(`[data-product-id="${productId}"]`);

    // Populate the modal fields
    document.getElementById('productModalImage').src = productCard.querySelector('img').src;
    document.getElementById('productModalPrice').textContent = productCard.querySelector('.card-text').textContent;
    document.getElementById('productModalDescription').textContent = `Description for ${productCard.querySelector('.card-title').textContent}`;

    // Show the modal without the backdrop
    let productModal = new bootstrap.Modal(document.getElementById('productModal'), {
      backdrop: false
    });
    productModal.show();
  }

  document.addEventListener('click', function (event) {
    var isClickInsideModal = document.querySelector('.modal-content').contains(event.target);
    var modalIsOpen = document.querySelector('.modal.show');

    if (modalIsOpen && !isClickInsideModal) {
      var modalElement = document.querySelector('.modal');
      var bootstrapModal = bootstrap.Modal.getInstance(modalElement);
      bootstrapModal.hide();
    }
  });

  document.querySelector('.maps').addEventListener('click', function (event) {
    event.preventDefault();
    document.getElementById('MapModals').style.display = 'block';
  });

  document.getElementById('closeModal').addEventListener('click', function () {
    document.getElementById('MapModals').style.display = 'none';
  });




</script>

